{
  "schemaVersion": "1.0",
  "plugin": {
    "id": "com.clapgo.gain",
    "name": "Simple Gain",
    "vendor": "ClapGo",
    "version": "1.0.0",
    "description": "A simple gain plugin using ClapGo",
    "url": "https://github.com/justyntemme/clapgo",
    "manualUrl": "https://github.com/justyntemme/clapgo",
    "supportUrl": "https://github.com/justyntemme/clapgo/issues",
    "features": ["audio-effect", "stereo", "mono"]
  },
  "build": {
    "dependencies": []
  },
  "extensions": [
    {
      "id": "clap.audio-ports",
      "supported": false
    },
    {
      "id": "clap.state",
      "supported": false
    },
    {
      "id": "clap.latency",
      "supported": true
    },
    {
      "id": "clap.tail",
      "supported": true
    },
    {
      "id": "clap.timer-support",
      "supported": true
    }
  ]
}