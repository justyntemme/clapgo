{
  "schemaVersion": "1.0",
  "plugin": {
    "id": "com.clapgo.synth",
    "name": "Simple Synth",
    "vendor": "ClapGo",
    "version": "1.0.0",
    "description": "A simple synthesizer using ClapGo",
    "url": "https://github.com/justyntemme/clapgo",
    "manualUrl": "https://github.com/justyntemme/clapgo",
    "supportUrl": "https://github.com/justyntemme/clapgo/issues",
    "features": ["instrument", "synthesizer", "stereo"]
  },
  "build": {
    "dependencies": []
  },
  "extensions": [
    {
      "id": "clap.audio-ports",
      "supported": true
    },
    {
      "id": "clap.note-ports",
      "supported": true
    },
    {
      "id": "clap.state",
      "supported": false
    }
  ],
  "parameters": [
    {
      "id": 1,
      "name": "Volume",
      "minValue": 0.0,
      "maxValue": 1.0,
      "defaultValue": 0.7,
      "flags": ["automatable", "bounded-below", "bounded-above"]
    },
    {
      "id": 2,
      "name": "Waveform",
      "minValue": 0.0,
      "maxValue": 2.0,
      "defaultValue": 0.0,
      "flags": ["automatable", "bounded-below", "bounded-above", "steppable"]
    }
  ]
}