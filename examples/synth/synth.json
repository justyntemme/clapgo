{
  "schemaVersion": "1.0",
  "plugin": {
    "id": "com.justyntemme.ClapGo.synth",
    "name": "ClapGo Simple Synth",
    "vendor": "ClapGo",
    "version": "1.0.0",
    "description": "A simple synthesizer with note name support",
    "url": "https://github.com/justyntemme/clapgo",
    "manualUrl": "https://github.com/justyntemme/clapgo",
    "supportUrl": "https://github.com/justyntemme/clapgo/issues",
    "features": [
      "instrument",
      "stereo",
      "mono"
    ]
  },
  "build": {
    "goSharedLibrary": "libsynth.so",
    "entryPoint": "CreatePlugin"
  },
  "extensions": [
    {
      "id": "clap.params",
      "supported": true
    },
    {
      "id": "clap.state",
      "supported": true
    },
    {
      "id": "clap.log",
      "supported": true
    },
    {
      "id": "clap.audio-ports",
      "supported": true
    },
    {
      "id": "clap.note-ports",
      "supported": true
    },
    {
      "id": "clap.voice-info",
      "supported": true
    },
    {
      "id": "clap.tuning",
      "supported": false
    },
    {
      "id": "clap.note-name",
      "supported": true
    },
    {
      "id": "clap.latency",
      "supported": true
    },
    {
      "id": "clap.tail",
      "supported": true
    },
    {
      "id": "clap.timer-support",
      "supported": true
    },
    {
      "id": "clap.context-menu",
      "supported": true
    }
  ]
}